        "astro": "npm:astro@^5.11.0",
        "clsx": "npm:clsx",
        "cookie": "npm:cookie",
        "cssesc": "npm:cssesc",
        "devalue": "npm:devalue",
        "es-module-lexer": "npm:es-module-lexer",
        "html-escaper": "npm:html-escaper",
